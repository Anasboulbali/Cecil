{
  "chmod": "0755",
  "directories": [
    "config",
    "src",
    "skeleton"
  ],
  "files": [
    "LICENSE",
    "skeleton/.router.php",
    "skeleton/.watch.js"
  ],
  "finder": [
    {
      "name": [
        "LICENSE*",
        "License*",
        "*.php",
        "*.xml",
        "*.xsd",
        "*.yaml",
        "*.yml",
        "*.json"
      ],
      "notName": [
        "composer.json"
      ],
      "exclude": [
        "phpunit",
        "Tests",
        "tests"
      ],
      "in": [
        "vendor"
      ]
    }
  ],
  "git-version": "git_version",
  "main": "bin/phpoole",
  "output": "dist/phpoole-v@git-version@.phar",
  "stub": true
}
